package snakeLab;

public class Driver {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Snake s = new Snake(0, 9, 9, null);
		System.out.println(s);
		s.update("UFMFMFMFMLFMDFMFMFMFMLFMUFMFMFMFMLFMDFMFMFMFMLMMMMMMUMMMMMMMMMRMDMMMMMRMUMMMMMR");
		s.reverse();
		System.out.println(s);
		System.out.println(s.ateFruit);
	}

}
