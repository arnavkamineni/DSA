package snakeLab;

public class Body {
	int x, y;
	Body next;
	
	public Body(int x, int y, Body next) {
		super();
		this.x = y;
		this.y = x;
		this.next = next;
	}

	public Body(int row, int col) {
		// TODO Auto-generated constructor stub
		this.y = row;
		this.x = col;
		next = null;
	}

	public Body getNext() {
		return next;
	}

	public void setNext(Body next) {
		this.next = next;
	}
	
	

}
